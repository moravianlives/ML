      <body xml:lang="en">
         
         <listPlace><!-- Fulneck -->
            <place xml:id="mlpla000003">
               <location>
                  <geo>53.7837, -1.6648</geo> 
               </location>
               <placeName type="standard">
                  <name>Fulneck</name>
               </placeName>
               <placeName type="preferred" xml:lang="en">
                  <name type="full">Fulneck Moravian Settlement</name>
                  <respons locus="name">
                     <desc>Moravian Lives full/preferred name</desc>
                  </respons>
               </placeName>
               <placeName type="variant" xml:lang="en" when-iso="1743">
                  <name type="moravian">Lambshill</name>
                  <note>
                     <label>Zinzendorf's preferred name for Fulneck</label>
                  </note>
               </placeName>
               <placeName type="variant" xml:lang="en">
                  <name type="deprecated">Fulnec</name>
                  <note>
                     <label>Alternate spelling</label>
                  </note>
               </placeName>
               <placeName type="variant" xml:lang="en">
                  <name type="historical">The Fall Necke</name>
                  <note>
                     <label>Old English term</label>
                  </note>
               </placeName>
               <placeName type="variant" xml:lang="en">
                  <name type="moravian">Fulnek</name>
                  <note>
                     <label>Named in honour of the Fulnek settlement in Moravia</label>
                  </note>
               </placeName>
               
               <event type="establishment" source="https://www.w3.org/TR/NOTE-datetime" when-iso="1744" cert="high" >
                  <desc>Note about the establishment of Fulneck</desc><!-- This would be an editorial note about ML's understanding of Fulneck -->
               </event>                                    

               <idno type="VIAF" cert="high" sameAs="6558159035174601380001"/> 
               <!--VIAF place ID is specifically Fulneck Moravian Church -->
               <idno type="Wikidata" cert="low" corresp="#Q5508369" /> 
               <!-- the Wikipedia entry is titled "Fulneck Moravian Settlement" but includes information beyond Moravian scope  -->
               
            </place>
