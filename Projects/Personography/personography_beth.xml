         </listPerson>
         <listPerson>
            <person xml:id="mlper000004" sex="f">
               <persName>Benigna Antes </persName>
               <birth>
                  <date when-iso="1748-10-04">1748-10-04</date>
                  <placeName>Bethlehem, Pennsylvania</placeName>
