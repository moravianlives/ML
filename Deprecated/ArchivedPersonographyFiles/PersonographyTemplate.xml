        <body>
            <listPerson>
                <person xml:id="mlper000215" sex="f">
                    <persName>
                        <forename>Anna <addName>Rosina</addName></forename>
                        <surname type="birth">Anderson</surname>
                        <name type="variant">Anders</name>
                    </persName>
                    <bibl type="memoir">Ful/SS/8</bibl>
                    <birth when-iso="1727-08-09">
                        <placeName ref="https://www.wikidata.org/wiki/Q827807">Berthelsdorf, Germany</placeName>
                    </birth>
                    <death when-iso="1803-02-20">
                        <placeName ref="https://www.wikidata.org/wiki/Q5508369">Sisters' House Fulneck, UK</placeName>
                    </death>
                    <event type="baptism" when-iso="1738">
                        <desc>
                            <placeName ref="https://www.wikidata.org/wiki/Q667587">Lutheran Church</placeName>
                        </desc>
                    </event>
                    <event type="baptism" when-iso="1739">
                        <desc>
                            <placeName ref="https://www.wikidata.org/wiki/Q667587">Sisters' House Marienborn</placeName>
                        </desc>
                    </event>
                    <event type="reception" when-iso="1740">
                        <desc>
                            Reception into Congregation
                            <placeName ref="https://www.wikidata.org/wiki/Q667587">Marienborn, Germany</placeName>
                        </desc>
                    </event>
                    <event type="sacrament" when-iso="1741">
                        <desc>
                            <placeName ref="https://www.wikidata.org/wiki/Q1614322">Herrnhaag, Germany</placeName>
                        </desc>
                    </event>
                    <occupation></occupation>
                    <office></office>
                </person>
                <listRelation>
                    <!-- For all of these relattitons, it is stating that Anna Rosina Anderson
                        is what the relation is refering to.
                    
                    ie: Anna Rosina Anderson is the sibling of #mlperxxx1 and #mlperxxx2 ...
                        Anna Rosina Anderson is the parent of #mlperxxx3
                        Anna Rosina Anderson is the child of #mlperxxx4 and #mlperxxx5
                        Anna Rosina Anderson is the spouse of #mlperxxx6-->
                    <relation name="sibling" active="#mlper000215" passive="#mlperxxx1 #mlperxxx2"/>
                        
                    
                    <relation name="parent" active="#mlper000215" passive="#mlperxxx3"/>
                        
                    
                    <relation name="child" active="#mlper000215" passive="#mlperxxx4 #mlperxxx5"/>
                        
                    
                    <relation name="spouse" active="#mlper000215" passive="#mlperxxx6"/>
                        
                    
                </listRelation>
            </listPerson>
